body {
    background: #d85940;
}

.background {
    background: #d85940;
    position: fixed;
    top: 0;
    width: 100%;
    height: 100%;
}

page {
    height: 100%;
    background: #d85940;
    overflow: hidden;
}

.order-fixed {
    padding: 0 40rpx;
}

.order-num {
    padding-bottom: 16rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #fff;
    font-size: 36rpx;
    height: 73rpx;
}

.num-info {
    display: flex;
    font-weight: bold;
}

.cancel-box {
    width: 320rpx;
    height: 92rpx;
    z-index: 9;
    border-radius: 24rpx;
    margin: 20rpx auto 0;
    position: absolute;
    bottom: 190rpx;
    left: 50%;
    transform: translateX(-50%);
}

.cancel-box-x {
    padding-bottom: 90rpx;
}

.change-btn {
    width: 100%!important;
    height: 100%!important;
}

.order-box {
    border-radius: 50%;
    background: white;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.order-text {
    display: inline-block;
    width: 50rpx;
    height: 50rpx;
    color: #f0bd86;
    font-size: 24rpx;
    line-height: 50rpx;
    border-radius: 50%;
    text-align: center;
    border: 1px #f0bd86 solid;
}

.line {
    width: 20rpx;
    height: 2rpx;
    background: white;
    display: inline-block;
}

.active {
    background: #f0bd86;
    color: white;
}

.current {
    background: rgb(192,64,48)!important;
}

.qa-area {
    top: 0;
    left: 0;
    right: 0;
    padding: 32rpx;
    padding-top: 120rpx;
    width: auto;
    background: #d85940;
    z-index: 0;
}

.bottom-x-qa {
    padding-bottom: 182rpx;
}

.qa-area text {
    font-size: 28rpx;
    display: block;
}

.qa-des {
    margin-top: 28rpx;
    padding: 12rpx;
    background: white;
    display: block;
    margin: auto;
}

.qa-des text {
    font-size: 28rpx;
    font-weight: 500;
}

.qa-des image {
    width: 100%;
    height: 331rpx;
}

.qa-options {
    display: block;
    padding: 12rpx;
    font-size: 28rpx;
    background: white;
    margin: auto;
    margin-top: 20rpx;
}

.qa-options-select {
    background: #f5e2ba;
}

.qa-options-hover {
    background: #f5e2ba;
}

.change-qa {
    overflow: hidden;
    white-space: nowrap;
    color: white;
    font-size: 24rpx;
    background: rgba(0,0,0,0.7);
    padding: 0 8rpx;
    border: 2rpx white solid;
    display: inline-block;
}

.change-qa-container {
    margin-top: -95rpx;
    right: 42rpx;
    position: absolute;
    z-index: 9;
    width: 100%;
}

.change-qa-box {
    padding: 20rpx 12rpx;
    display: inline-block;
    float: right;
}

.submit-btn {
    background-color: rgb(238,205,145)!important;
    color: rgb(192,64,48)!important;
    border-radius: 45rpx;
    font-size: 34rpx!important;
    border: 0!important;
}

.button-area {
    padding: 32rpx 0;
    padding-bottom: 0;
}

.textarea-box {
    padding: 24rpx;
    background: white;
}

.self-question-box {
    background: white;
    font-size: 32rpx;
    display: block;
    padding: 0;
    width: auto;
    height: 100rpx;
}

.add-pic-box {
    padding: 12rpx 24rpx;
    background: white;
    display: inline-block;
    font-size: 24rpx!important;
    margin-top: 24rpx;
    border-radius: 6rpx;
}

.pic-img {
    margin-top: 24rpx;
}

.pic-img image {
    width: 200rpx;
    height: 200rpx;
}

.option-tips {
    font-size: 32rpx;
    display: block;
    margin-top: 18rpx;
}

.add-options-area input {
    background: white;
    padding: 12rpx;
    width: 500rpx;
    height: 50rpx;
    font-size: 32rpx;
    float: left;
}

.tick-box {
    width: 60rpx;
    height: 60rpx;
    background: white;
    float: right;
    display: flex;
    justify-content: center;
    align-items: center;
}

.tick {
    width: 30rpx;
    height: 30rpx;
    display: inline-block;
}

.add-item {
    margin-top: 18rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.tick-box image {
    width: 40rpx;
    height: 40rpx;
}

.picker {
    background: white;
    padding: 12rpx;
    font-size: 28rpx;
    margin-top: 20rpx;
}

.picker-title {
    color: white;
    font-size: 28rpx;
    margin-top: 20rpx;
}

.picker-input {
    font-size: 28rpx;
    line-height: 32rpx;
    margin-top: 20rpx;
    padding: 8rpx 12rpx;
    display: inline-block;
    margin-left: 20rpx;
}

.picker-input-box {
    display: flex;
    align-items: center;
}

.picker-input-box input {
    flex: 1;
}

.picker-view {
    margin-top: 20rpx;
}

.other-options {
    width: 100%;
    bottom: 0;
    background: #d85940;
}

.button-active {
    background-color: #fff!important;
}

.bottom-x {
    padding-bottom: 64rpx;
}

.white-btn {
    outline: none;
    width: 560rpx;
    height: 88rpx;
    font-size: 32rpx!important;
    border-radius: 44rpx!important;
    background: linear-gradient(to bottom,#fff0e9,#ffdbc9)!important;
    box-shadow: 0 -4rpx 8rpx #fff inset,0 6rpx 10rpx #b64f36;
    padding: 0!important;
    line-height: 88rpx!important;
    border: 0!important;
    margin-top: 24rpx!important;
    margin-bottom: 24rpx!important;
    color: #e26649!important;
    display: flex!important;
    justify-content: center!important;
    align-items: center!important;
}

.yellow-btn {
    outline: none;
    width: 560rpx;
    height: 88rpx;
    font-size: 32rpx!important;
    border-radius: 44rpx!important;
    background: linear-gradient(to bottom,#fff9b1,#ffd973)!important;
    box-shadow: 0 -4rpx 8rpx #fff992 inset,0 6rpx 10rpx #b64f36;
    padding: 0!important;
    line-height: 88rpx!important;
    border: 0!important;
    margin-top: 24rpx!important;
    margin-bottom: 48rpx!important;
    color: #e26649!important;
    display: flex!important;
    justify-content: center!important;
    align-items: center!important;
}

.qa-section {
    margin-left: auto;
    margin-right: auto;
    padding: 0 40rpx;
    margin-top: 20rpx;
    position: relative;
}

.qa-section image {
    width: 100%;
    height: 340rpx;
}

.qa-box {
    position: relative;
    padding: 70rpx 73rpx 70rpx 65rpx;
    background: #fff;
    border-radius: 10rpx;
}

.be-selected-icon {
    width: 169rpx!important;
    height: 142rpx!important;
    position: absolute;
    top: -71rpx;
    right: 0;
}

.qa-question {
    color: #bb2b0f;
    font-size: 34rpx;
    font-weight: bold;
}

.radio-label {
    font-size: 28rpx;
    color: #5a5a5a;
    align-items: center;
    margin-top: 20rpx;
    line-height: 1.4;
    display: flex;
}

.radio-label image {
    margin-right: 14rpx;
    display: inline-block;
}

.radio-qa {
    display: flex;
    align-items: left;
    justify-content: space-around;
    flex-direction: column;
    height: 90rpx;
    border-radius: 45rpx;
    padding: 0 40rpx;
    border: 2rpx solid #ccc;
    color: #282828;
    transition: all 0.15s linear;
}

.radio-qa-select {
    border: 2rpx solid #d95940;
    background: #d95940;
    color: #fff;
}

.select-classify-item {
    position: relative;
    width: 180rpx;
    height: 72rpx;
    border-radius: 36rpx;
    font-size: 34rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #e99930;
    margin-top: 30rpx;
    margin-left: auto;
    margin-right: auto;
}

.end-classify-item {
    width: 180rpx;
    height: 72rpx;
    border-radius: 36rpx;
    font-size: 34rpx;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #ed4c38;
    margin-top: 30rpx;
    margin-left: auto;
    margin-right: auto;
}

.select-tips-box {
    width: 88rpx;
    height: 40rpx;
    border-radius: 20rpx;
    background: #ed4c38;
    font-size: 30rpx;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    position: absolute;
    z-index: 9;
    right: -4rpx;
    top: -24rpx;
}

.choose-btn {
    color: #b97215;
    font-size: 30rpx;
    margin-top: 30rpx;
}

.button-area {
    display: flex;
    position: absolute;
    bottom: 79rpx;
    width: 100%;
    justify-content: center;
}

.button-area button::after {
    border: none;
}

.button-area button {
    margin: 0 15rpx;
    padding: 0;
    font-size: 30rpx;
    color: #bb2b0f;
    background: #fff4e1;
    width: 180rpx;
    height: 72rpx;
    border-radius: 32rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}