.chip-container {
    position: absolute;
    z-index: 1;
    box-sizing: border-box;
    transform: perspective(1000px);
    transform-style: preserve-3d;
}

.chip-image {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    backface-visibility: hidden;
}

.chip-front {
    transform: rotateY(0deg);
}

.chip-front.flip {
    transform: rotateY(180deg);
}

.chip-back {
    transform: rotateY(-180deg);
}

.chip-back.flip {
    transform: rotateY(0deg);
}

.chip-selected {
    border: 1rpx solid rgb(192,64,48);
}

@-webkit-keyframes shake {
    0%,100% {
        transform: translateX(0);
    }

    10%,30%,50%,70%,90% {
        transform: translateX(-10rpx);
    }

    20%,40%,60%,80% {
        transform: translateX(10rpx);
    }
}

@keyframes shake {
    0%,100% {
        transform: translateX(0);
    }

    10%,30%,50%,70%,90% {
        transform: translateX(-10rpx);
    }

    20%,40%,60%,80% {
        transform: translateX(10rpx);
    }
}

.chip-shake {
    animation: shake 0.5s ease-in-out;
}

